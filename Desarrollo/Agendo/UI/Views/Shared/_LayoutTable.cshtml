<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Syspan</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/css/framework")
    @Styles.Render("~/Content/css/plugin")


</head>

<body class="skin-dark">
    <!-- BEGIN HEADER -->
    @Html.Partial("_LayoutHeader")
    <!-- END HEADER -->

    <div class="wrapper row-offcanvas row-offcanvas-left">
        <!-- BEGIN SIDEBAR -->
        @Html.Partial("_LayoutMenu")
        <!-- END SIDEBAR -->
        <!-- BEGIN CONTENT -->
        @RenderBody()
        <!-- END CONTENT -->
        <!-- BEGIN SCROLL TO TOP -->
        <div class="scroll-to-top"></div>
        <!-- END SCROLL TO TOP -->
    </div>
</body>
       
</html>
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)

<!-- BEGIN JS FRAMEWORK -->
<script src="~/Content/assets/plugins/jquery-2.1.0.min.js"></script>
<script src="~/Content/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
<!-- END JS FRAMEWORK -->
<!-- BEGIN JS PLUGIN -->
<script src="~/Content/assets/plugins/pace/pace.min.js"></script>
<script src="~/Content/assets/plugins/jquery-totemticker/jquery.totemticker.min.js"></script>
<script src="~/Content/assets/plugins/jquery-resize/jquery.ba-resize.min.js"></script>
<script src="~/Content/assets/plugins/jquery-blockui/jquery.blockUI.min.js"></script>
<script src="~/Content/assets/plugins/jquery-datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Content/assets/plugins/jquery-datatables/js/dataTables.bootstrap.js"></script>
<script src="~/Content/assets/js/datatables.js"></script>
<!-- END JS PLUGIN -->
<!-- BEGIN JS TEMPLATE -->
<script src="~/Content/assets/js/main.js"></script>
<script src="~/Content/assets/js/skin-selector.js"></script>
<!-- END JS TEMPLATE -->