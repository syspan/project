<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Syspan</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/css/framework")
    @Styles.Render("~/Content/css/plugin")


</head>

<body class="skin-dark">
    <!-- BEGIN HEADER -->
    @Html.Partial("_LayoutHeader")
    <!-- END HEADER -->
    <div class="wrapper row-offcanvas row-offcanvas-left">
        <!-- BEGIN SIDEBAR -->
        @Html.Partial("_LayoutMenu")
        <!-- END SIDEBAR -->
        <!-- BEGIN CONTENT -->
        @RenderBody()
        <!-- END CONTENT -->
        <!-- BEGIN SCROLL TO TOP -->
        <div class="scroll-to-top"></div>
        <!-- END SCROLL TO TOP -->
    </div>
</body>
    
</html>
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)

<!-- BEGIN JS FRAMEWORK -->
<script src="~/Content/assets/plugins/jquery-2.1.0.min.js"></script>
<script src="~/Content/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
<!-- END JS FRAMEWORK -->
<!-- BEGIN JS PLUGIN -->
<script src="~/Content/assets/plugins/pace/pace.min.js"></script>
<script src="~/Content/assets/plugins/jquery-totemticker/jquery.totemticker.min.js"></script>
<script src="~/Content/assets/plugins/jquery-resize/jquery.ba-resize.min.js"></script>
<script src="~/Content/assets/plugins/jquery-blockui/jquery.blockUI.min.js"></script>
<script src="~/Content/assets/plugins/jquery-datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Content/assets/plugins/jquery-datatables/js/dataTables.bootstrap.js"></script>
<script src="~/Content/assets/js/datatables.js"></script>

<script src="~/Content/assets/plugins/jquery-gritter/js/jquery.gritter.min.js"></script>
<script src="~/Content/assets/plugins/jquery-sparkline/jquery.sparkline.min.js"></script>
<!--[if lte IE 8]>
    <script language="javascript" type="text/javascript" src="assets/plugins/jquery-flot/excanvas.min.js"></script>
<![endif]-->
<script src="~/Content/assets/plugins/jquery-flot/jquery.flot.min.js"></script>
<script src="~/Content/assets/plugins/jquery-flot/jquery.flot.labels.min.js"></script>
<script src="~/Content/assets/plugins/jquery-flot/jquery.flot.resize.min.js"></script>
<script src="~/Content/assets/plugins/jquery-flot/jquery.flot.orderBars.min.js"></script>
<script src="~/Content/assets/plugins/jquery-flot/jquery.flot.pie.min.js"></script>
<script src="~/Content/assets/plugins/jquery-flot/curvedLines.js"></script>
<script src="~/Content/assets/plugins/skycons/skycons.js"></script>
<script src="~/Content/assets/plugins/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js"></script>
<script src="~/Content/assets/plugins/jquery-jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
<script src="~/Content/assets/plugins/jquery-jvectormap/jquery-jvectormap-us-aea-en.js"></script>
<script src="~/Content/assets/js/dashboard.js"></script>


<!-- END JS PLUGIN -->
<!-- BEGIN JS TEMPLATE -->
<script src="~/Content/assets/js/main.js"></script>
@*<script src="~/Content/assets/js/skin-selector.js"></script>*@
<!-- END JS TEMPLATE -->