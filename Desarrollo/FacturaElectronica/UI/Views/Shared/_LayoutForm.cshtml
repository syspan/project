    <script src="~/Content/assets/plugins/bootstrap-summernote/summernote.min.js"></script>
    <script src="~/Content/assets/plugins/rut/jquery.Rut.js"></script>

    

    <script type="text/javascript">
        /* SUMMERNOTE WYSIWYG */
        jQuery('#summernote').summernote({
            height: 300
        });

        jQuery('#click2edit').click(function () {
            $(this).summernote({
                focus: true,
                onblur: function (e) {
                    $('#click2edit').destroy();
                }
            });
        });

        $('#airmode').summernote({
            height: 300,
            airMode: true
        });

        $('#Rut').Rut();


        $('.focus :input').focus();


    </script>
 

